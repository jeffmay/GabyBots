var TWP=window.TWP||TWP;TWP.setAutocomplete=function(B,A){var C=this,B=(!B)?$("form").eq(0):$(B),A=(!A)?B.find("input[type=text]").eq(0):$(A);A.autocomplete({source:function(E,D){$.ajax({url:"http://www.washingtonpost.com/ssearch/spellsuggestcore/select/?json.wrf=?",dataType:"jsonp",data:{q:"suggest:"+$.trim(E.term.toLowerCase())+"*",start:"0",rows:"0",facet:"true","facet.method":"enum","facet.field":"suggest","facet.limit":"10","facet.prefix":$.trim(E.term.toLowerCase()),omitHeader:"true",wt:"json"},success:function(F){D($.map(F.facet_counts.facet_fields.suggest,function(G){if(typeof (G)==="string"){return{label:G,value:G};}}));}});},minLength:2,select:function(D){$("input.global-search",B).trigger("click");},focus:function(E,D){B.find("#query").val(D.label);},open:function(D){$(".ui-autocomplete .ui-menu-item").delegate("a","mouseover",function(E){B.find("input[type=text]").val($(this).text());});}});};$(document).ready(function(){try{TWP.setAutocomplete($("form","#hot-topics-wrapper"));}catch(A){}});