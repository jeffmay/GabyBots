(function(A){A.fn.applesimulator=function(B){return this.each(function(){new A.applesimulator(this,B);});};A.applesimulator=function(C,B){this.el=A(C).get(0);this.options=B||{};this.options=A.extend(B);this.props={};this.elems={};this.init();};A.applesimulator.fn=A.applesimulator.prototype={};A.applesimulator.fn.extend=A.applesimulator.extend=jQuery.extend;A.applesimulator.fn.extend({init:function(){this.cacheElems();this.assignHandlers();},cacheElems:function(){this.elems.$icons=A(".icon-large",this.el);this.elems.$shadow=A(".icon-shadow",this.el);this.elems.$textHook=A(".slogan",this.el);this.elems.$textDiv=A(".r1-list",this.el);this.elems.$bottomCap=A(".sim-bottom",this.el);},assignHandlers:function(){that_=this;this.elems.$icons.hover(function(B){that_.over(B);that_.swapText(B);that_.moveCap(B);},function(B){that_.out(B);});},over:function(B){var C=A(B.target.parentNode);A(B.target).animate({"top":"-5px"},"fast");C.siblings().animate({"top":"33px"},"fast");},out:function(B){var C=A(B.target.parentNode);A(B.target).animate({"top":"0"},"fast");C.siblings().animate({"top":"28px"},"fast");},swapText:function(D){var B=A(D.target).attr("rel");var C=this.elems.$textDiv.find("p[rel='"+B+"'] a").clone();this.elems.$textHook.empty().append(C);},moveCap:function(B){var C=A(B.target).parent().parent().position();this.elems.$bottomCap.css("left",C.left+4);}});})(jQuery);