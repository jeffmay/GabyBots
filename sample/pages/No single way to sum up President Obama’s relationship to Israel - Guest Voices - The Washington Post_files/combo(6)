(function(A){A.fn.liveQAScrollable=function(B){return this.each(function(){var D=A(this);var F=(function(){return(D.attr("class").indexOf("nowDiv")!=-1);})();var E=A(".nowDiv").find("tr").size();var G=0;if(!F){G=D.find("tr").size();}var C=0;if(F){if(E>3){D.find("tr:lt(3)").each(function(H){return C=C+A(this).height();});D.css({"border-top":"1px solid #DEDEDE","margin-bottom":"3px","height":C,"overflow-y":"auto"});}}else{if(E<=2){if(G>3){D.find("tr:lt(3)").each(function(H){return C=C+A(this).height();});D.find("tr").each(function(H){A(this).addClass("border-bottom");});D.css({"height":C,"overflow-y":"auto","margin":"3px -5px 0 0","border-top":"1px solid #DEDEDE"});D.parent().css({"border":"none","margin-bottom":"3px"});}}}});};})(jQuery);